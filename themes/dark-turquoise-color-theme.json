	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#116062"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#deb887"
			}
		},
		{
			"name": "Template Definition",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#da70d6"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#deb887"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5ec4cd"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#f5fffa"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#f5fffa"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#1faee9"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#c0c0c0"
			}
		},
		{
			"name": "Quotes",
			"scope": "punctuation.definition.string",
			"settings": {
				"foreground": "#f53d68"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f53d68"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#50c878"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#50c878"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#f53d68"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#f5fffa"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#fbec5d"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#1faee9"
			}
		},
		{
			"name": "Tag brackets",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#1faee9"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#50c878"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#5ec4cd"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f53d68"
			}
		},
		{
			"name": "Library class",
			"scope": "support.class",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5ec4cd"
			}
		},
		{
			"name": "Library type",
			"scope": "support.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5ec4cd"
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#deb887"
			}
		},
		{
			"name": "Exceptions",
			"scope": [
				"keyword.operator.new"
			],
			"settings": {
				"foreground": "#5ec4cd"
			}
		},
		{
			"name": "this-self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5ec4cd"
			}
		}
	]
}